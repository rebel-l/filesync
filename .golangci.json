{
    "issues": {
        "exclude-rules": [],
        "exclude-use-default": false,
        "max-issues-per-linter": 0,
        "max-same-issues": 0
    },
    "linters": {
        "enable-all": true,
        "disable": [
            "godox",
            "gofumpt",
            "golint",
            "goerr113",
            "scopelint",
            "stylecheck"
        ]
    },
    "run": {
        "deadline": "5m",
        "skip-dirs": [
            "reports",
            "scripts"
        ],
        "tests": true
    }
}
